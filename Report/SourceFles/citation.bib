@article{LEE1997533,
title = {Dullrazor®: A software approach to hair removal from images},
journal = {Computers in Biology and Medicine},
volume = {27},
number = {6},
pages = {533-543},
year = {1997},
issn = {0010-4825},
doi = {https://doi.org/10.1016/S0010-4825(97)00020-6},
url = {https://www.sciencedirect.com/science/article/pii/S0010482597000206},
author = {Tim Lee and Vincent Ng and Richard Gallagher and Andrew Coldman and David McLean},
keywords = {Hair removal, Skin imaging, Image processing, Grayscale morphology Adaptive median filter},
abstract = {Recently, there has been a growing number of studies applying image processing techniques to analyze melanocytic lesions for atypia and possible malignancy and for total-body mole mapping. However, such lesions can be partially obscured by body hairs. None of these studies has fully addressed the problem of human hairs occluding the imaged lesions. In our previous study we designed an automatic segmentation program to differentiate skin lesions from the normal healthy skin, and learned that the program performed well with most of the images, the exception being those with hairs, especially dark thick hairs, covering part of the lesions. These thick dark hairs confused the program, resulting in unsatisfactory segmentation results. In this paper, we present a method to remove hairs from an image using a pre-processing program we have called DullRazor®. This pre-processing step enables the segmentation program to achieve satisfactory results. DullRazor® can be downloaded as shareware from http://www.derm.ubc.ca.}
}

